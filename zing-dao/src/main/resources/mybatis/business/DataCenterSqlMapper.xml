<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yibao.dao.mapper.business.DataCenterSqlMapper">

    <select id="executeSql" resultType="java.util.Map" parameterType="java.lang.String">
        ${sql}
    </select>
    <select id="selectByBaseCondition" resultMap="com.yibao.dao.mapper.base.DataCenterSqlBaseMapper.BaseResultMap"
    parameterType="java.util.Map">
        SELECT
            <include refid="com.yibao.dao.mapper.base.DataCenterSqlBaseMapper.BaseColumn"/>
        FROM `yb_data_center_sql`
        <include refid="com.yibao.dao.mapper.base.DataCenterSqlBaseMapper.QueryCondition"/>
    </select>
</mapper>